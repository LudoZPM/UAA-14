<Window x:Class="_6T24_ludo_FrameEx1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_6T24_ludo_FrameEx1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition Height="1*"></RowDefinition>
        </Grid.RowDefinitions>
        <Button  Click="Page1_Click" x:Name="page1" Margin="10,0,10,0" Width="270" Height="100" Foreground="red" Background="LightGoldenrodYellow" FontStyle="Italic" FontSize="20" FontWeight="Bold" Content="Page 1" HorizontalAlignment="Left" VerticalAlignment="Top" ></Button>
        <Button Click="Page2_Click" x:Name="page2" Width="300" Height="100"  Foreground="red"  Background="LightGoldenrodYellow" FontStyle="Italic" FontSize="20" FontWeight="Bold" Content="Page 2" HorizontalAlignment="Center" VerticalAlignment="Top"  ></Button>
        <TextBlock Foreground="Black" FontSize="22" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center">Page d Acceuille</TextBlock>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" x:Name="Main" />
    </Grid>
</Window>
